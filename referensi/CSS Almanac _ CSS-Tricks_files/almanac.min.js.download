$(".selector-list .page_item > a").each((l,a)=>{const e=$(a),t=e.attr("href").split("/")[5];e.attr("id","almanac-selector-"+t)}),$("#almanac-sidebar-list > li > ul > li").each((l,a)=>{0===$(a).find("ul").length&&$(a).hide()}),$("#almanac-sidebar-list > li > ul > li > a").on("click",l=>{l.preventDefault(),$(l.target).toggleClass("almanac-sidebar-link-open").closest("li").find("ul").toggle()});